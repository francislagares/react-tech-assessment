{
  "name": "react-typescript-vite",
  "private": true,
  "version": "v1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "preview": "vite preview",
    "test:ci": "vitest run",
    "test:watch": "vitest watch --ui",
    "test:coverage": "vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext js,jsx,ts,tsx --fix --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|json|md)\" --write",
    "stylelint": "stylelint **/*.{css,scss,tsx,js} --fix",
    "prepare": "husky install",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@mdx-js/react": "^3.0.1",
    "@storybook/addon-actions": "^7.6.16",
    "@storybook/addon-docs": "^7.6.16",
    "@storybook/addon-essentials": "^7.6.16",
    "@storybook/addon-interactions": "^7.6.16",
    "@storybook/addon-links": "^7.6.16",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.16",
    "@storybook/builder-vite": "^7.6.16",
    "@storybook/react": "^7.6.16",
    "@storybook/react-vite": "^7.6.16",
    "@storybook/test": "^7.6.16",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.0",
    "@vitest/ui": "1.3.0",
    "commitizen": "^4.3.0",
    "cypress": "^13.6.4",
    "cypress-vite": "^1.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-css-import-order": "^1.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^9.0.11",
    "jest-axe": "^8.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.1",
    "msw-storybook-addon": "^1.10.0",
    "prettier": "^3.2.5",
    "prettier-stylelint": "^0.4.2",
    "prop-types": "^15.8.1",
    "storybook": "^7.6.16",
    "stylelint": "^16.2.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.0",
    "vitest-axe": "^0.1.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=21.5.0",
    "npm": ">=10.2.5"
  },
  "packageManager": "pnpm@8.15.0"
}
